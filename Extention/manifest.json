{
  "manifest_version": 2,
  "name": "MindwaveKZ",
  "version": "1.0.1",
  "description": "An extention for eyes tracking on websites",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webgazer.js", "script.js"],
      "css": ["styles.css"]
    }
  ],  
  "icons":{"512": "logo.png"},
  "permissions": ["storage"]
}
